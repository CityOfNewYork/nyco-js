{
  "name": "nyco-patterns-framework",
  "version": "0.0.0-alpha",
  "description": "Front-end utilities from the Mayor's Office for Economic Opportunity",
  "main": "scripts/cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfNewYork/nyco-js.git"
  },
  "author": "NYC Opportunity <technology@nycopportunity.nyc.gov>",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/CityOfNewYork/nyco-js/issues"
  },
  "homepage": "https://github.com/CityOfNewYork/nyco-js#readme",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "ajv": "^6.4.0",
    "autoprefixer": "^8.2.0",
    "browser-sync": "^2.24.7",
    "concurrently": "^4.1.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "cssnano-cli": "^1.0.5",
    "directory-tree": "^2.1.0",
    "escape-html": "^1.0.3",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "express": "^4.16.3",
    "gh-pages": "^2.0.1",
    "json-sass": "^1.3.5",
    "lodash-es": "^4.17.8",
    "markdown": "^0.5.0",
    "mkdirp": "^0.5.1",
    "node-emoji": "^1.10.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.7",
    "postcss": "^6.0.21",
    "pretty": "^2.0.0",
    "promise-polyfill": "^7.1.1",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.3.1",
    "slm": "^1.0.0",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "svgo": "^1.0.5",
    "svgstore-cli": "^1.3.1",
    "tailwindcss": "^1.0.5",
    "uglify-js": "^3.3.16",
    "vue-template-compiler": "^2.6.9"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development cross-env PORT=7070 concurrently \"pttrn default -w\" \"pttrn serve\" \"pttrn sync\" -p \"none\"",
    "start:production": "cross-env NODE_ENV=production cross-env PORT=7070 concurrently \"pttrn default -w\" \"pttrn serve\" \"pttrn sync\" -p \"none\"",
    "prepack": "cross-env NODE_ENV=production pttrn default",
    "publish": "cross-env NODE_ENV=production pttrn build && cross-env NODE_ENV=production pttrn publish"
  },
  "bin": {
    "pttrn": "scripts/cli.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "false",
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "FB": false
    },
    "rules": {
      "no-console": 1,
      "one-var": 0,
      "comma-dangle": 0,
      "curly": [
        "error",
        "multi"
      ]
    }
  }
}
